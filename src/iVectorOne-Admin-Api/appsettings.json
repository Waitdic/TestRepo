{
  "Debug": {
    "EnableLogging": true,
    "EnableAuditing": true
  },
  "Serilog": {
    "Using": [
      "AWS.Logger.SeriLog"
    ],
    "LogGroup": "iVectorOneAdmin",
    "Region": "eu-west-1",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "EndsWith(RequestPath, '/healthcheck')" }
      }
    ],
    "WriteTo": [
      {
        "Name": "AWSSeriLog",
        "Args": {
          "textFormatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "ConfigConnection": "server=172.17.3.40;database=ThirdPartyImport;connect timeout=100;uid=ivector;pwd=_Trek1200;Encrypt=False",
    "AuditDatabase": "server=172.17.3.40;database=ThirdPartyImport;connect timeout=100;uid=ivector;pwd=_Trek1200;Encrypt=False"
  },
  "EnableSwagger": false,
  "Cors": {
    "AllowedOrigins": [
      "*"
    ],
    "AllowedMethods": [
      "GET",
      "POST",
      "DELETE",
      "PUT",
      "OPTIONS"
    ],
    "AllowedHeaders": [
      "*"
    ]
  },
  "SearchService": "https://api.ivectorone.com/"
}