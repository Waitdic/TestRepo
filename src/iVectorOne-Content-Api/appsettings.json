{
  "ConnectionStrings": {
    "ConnectionString": "server=172.17.3.40;database=ThirdPartyImport;connect timeout=100;uid=ivector;pwd=_Trek1200",
    "Default": "server=172.17.3.40;database=ThirdPartyImport;connect timeout=100;uid=ivector;pwd=_Trek1200"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "@mt = 'An unhandled exception has occurred while executing the request.'" }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "c:/logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  },
  "AllowedHosts": "*",
  "Scheduling": {
    "Enabled": true,
    "Jobs": {
      "iVectorOne Import": {
        "Trigger": "Cron",
        "Cron": "0 0 0 ? * * *"
      },
      "Own Stock Import": {
        "Trigger": "Cron",
        "Cron": "0 0/5 * ? * * *"
      }
    }
  }
}