{
  "Debug": {
    "EnableLogging": true,
    "EnableAuditing": false
  },
  "Serilog": {
    "Using": [
      "AWS.Logger.SeriLog"
    ],
    "LogGroup": "iVectorOneAdmin",
    "Region": "eu-west-1",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "AWSSeriLog",
        "Args": {
          "textFormatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "ConfigConnection": "server=172.17.3.40;database=ThirdPartyImport;connect timeout=100;uid=ivector;pwd=_Trek1200;",
    "TelemetryDatabase": "Server=localhost\\SQLEXPRESS;Database=Telemetry;Integrated Security=SSPI;"
  },
  "EnableSwagger": false,
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "http://localhost:8080",
      "http://localhost:80"
    ],
    "AllowedMethods": [
      "GET",
      "POST",
      "DELETE",
      "OPTIONS"
    ],
    "AllowedHeaders": [
      "X-Request-Token",
      "Accept",
      "Content-Type",
      "Authorization"
    ]
  }
}